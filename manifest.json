{
    "manifest_version": 2,

    "name": "Search Stack Overflow",
    "description": "Google search filtering by site:StackOverflow",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
